{
  "name": "@me-mudei/core",
  "version": "0.0.1",
  "main": "index.js",
  "imports": {
    "#shared/*": "./dist/core/shared/*.js",
    "#property/*": "./dist/core/property/*.js",
    "#user/*": "./dist/core/user/*.js"
  },
  "exports": {
    "./shared/application": "./dist/shared/application/index.js",
    "./shared/domain": "./dist/shared/domain/index.js",
    "./shared/infra": "./dist/shared/infra/index.js",
    "./user/application": "./dist/user/application/index.js",
    "./user/domain": "./dist/user/domain/index.js",
    "./user/infra": "./dist/user/infra/index.js",
    "./user": "./dist/user/index.js",
    "./property/application": "./dist/property/application/index.js",
    "./property/domain": "./dist/property/domain/index.js",
    "./property/infra": "./dist/property/infra/index.js",
    "./property": "./dist/property/index.js"
  },
  "typesVersions": {
    ">=4.0": {
      "shared/application": [
        "./dist/shared/application/index.d.ts"
      ],
      "shared/domain": [
        "./dist/shared/domain/index.d.ts"
      ],
      "shared/infra": [
        "./dist/shared/infra/index.d.ts"
      ],
      "user/application": [
        "./dist/user/application/index.d.ts"
      ],
      "user/domain": [
        "./dist/user/domain/index.d.ts"
      ],
      "user/infra": [
        "./dist/user/infra/index.d.ts"
      ],
      "user": [
        "./dist/user/index.d.ts"
      ],
      "property/application": [
        "./dist/property/application/index.d.ts"
      ],
      "property/domain": [
        "./dist/property/domain/index.d.ts"
      ],
      "property/infra": [
        "./dist/property/infra/index.d.ts"
      ],
      "property": [
        "./dist/property/index.d.ts"
      ]
    }
  },
  "scripts": {
    "ts-node": "ts-node",
    "tsc": "tsc",
    "tsc:check": "npm run tsc -- --noEmit",
    "build": "tsc -b ./tsconfig.json",
    "build:w": "tsc -b ./tsconfig.json -w",
    "clean:tsc": "rm tsconfig.tsbuildinfo",
    "test": "jest",
    "test:cov": "npm run test -- --coverage",
    "cti": "cti",
    "cti:make": "sh ./cti.sh",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@swc/core": "^1",
    "@swc/jest": "^0",
    "@types/jest": "^29.1.1",
    "@types/lodash": "^4",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "create-ts-index": "^1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29",
    "ts-node": "^10",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4",
    "aws-sdk": "^2.1238.0",
    "class-validator": "^0.13.2",
    "nanoid": "^3.3.4",
    "nexus": "^1",
    "prisma": "^4",
    "winston": "^3.8.2"
  },
  "author": "Uriel Guy <urielsateles@gmail.com>"
}
