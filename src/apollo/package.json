{
  "name": "@me-mudei/apollo",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/apollo",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "tsc:check": "npm run tsc -- --noEmit",
    "build": "tsc -b ./tsconfig.json",
    "build:w": "tsc -b ./tsconfig.json -w",
    "clean:tsc": "rm tsconfig.tsbuildinfo",
    "test": "jest",
    "test:cov": "npm run test -- --coverage",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@swc/core": "^1",
    "@swc/jest": "^0",
    "@types/aws-lambda": "^8",
    "@types/jest": "^28.1.8",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-node": "^10",
    "ts-node-dev": "^2",
    "typescript": "^4"
  },
  "dependencies": {
    "apollo-server": "^3",
    "apollo-server-core": "^3",
    "apollo-server-lambda": "^3",
    "graphql": "^15",
    "graphql-middleware": "^6",
    "graphql-scalars": "^1",
    "graphql-shield": "^7"
  },
  "author": "Uriel Guy <urielsateles@gmail.com>"
}
