{
  "name": "@me-mudei/apollo",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "tsc:check": "npm run tsc -- --noEmit",
    "build": "tsc -b ./tsconfig.json",
    "build:w": "tsc -b ./tsconfig.json -w",
    "clean:tsc": "rm tsconfig.tsbuildinfo",
    "test": "jest",
    "test:cov": "npm run test -- --coverage",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@swc/core": "^1",
    "@swc/jest": "^0",
    "@types/aws-lambda": "^8",
    "@types/jest": "^29.1.1",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-node": "^10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "apollo-server": "^3",
    "apollo-server-core": "^3",
    "apollo-server-lambda": "^3",
    "graphql": "^16",
    "graphql-scalars": "^1",
    "nanoid": "^3.3.4",
    "nexus-shield": "^2.2.1",
    "winston": "^3.8.2"
  },
  "author": "Uriel Guy <urielsateles@gmail.com>"
}
