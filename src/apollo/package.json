{
  "name": "@me-mudei/apollo",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "npm run generate && ts-node-dev --no-notify --respawn --transpile-only src",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "tsc:check": "npm run tsc -- --noEmit",
    "build": "npm run generate && tsc -b ./tsconfig.json",
    "build:w": "npm run generate && tsc -b ./tsconfig.json -w",
    "clean:tsc": "rm tsconfig.tsbuildinfo",
    "generate": "ts-node --transpile-only -e 'import NexusSchema from \"./src/schema\"; NexusSchema.makeSchema();'",
    "test": "npm run generate && jest",
    "test:cov": "npm run test -- --coverage",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@swc/core": "^1",
    "@swc/jest": "^0",
    "@types/aws-lambda": "^8",
    "@types/graphql-upload": "^8.0.12",
    "@types/jest": "^29.4.0",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-node": "^10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@apollo/server": "^4.0.2",
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.0",
    "@as-integrations/aws-lambda": "^1.0.1",
    "express": "^4.18.2",
    "file-type": "^18.0.0",
    "graphql": "^16",
    "graphql-scalars": "^1.19.0",
    "graphql-upload": "^13.0.0",
    "nanoid": "^3.3.4",
    "nexus-shield": "^2.2.1",
    "winston": "^3.8.2"
  },
  "author": "Uriel Guy <urielsateles@gmail.com>"
}
