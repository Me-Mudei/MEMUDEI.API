{
  "name": "me-mudei-api",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "directories": {
    "doc": "docs"
  },
  "imports": {
    "#shared/*": "./src/shared/*/index.js",
    "#property/*": "./src/property/*/index.js",
    "#adm/*": "./src/adm/*/index.js",
    "#auth/*": "./src/auth/*/index.js",
    "#apollo/*": "./src/apollo/*/index.js",
    "#user/*": "./src/user/*/index.js",
    "#schedule/*": "./src/schedule/*/index.js"
  },
  "exports": {
    "./shared": "./src/shared/index.js",
    "./user": "./src/user/index.js",
    "./schedule": "./src/schedule/index.js",
    "./property": "./src/property/index.js",
    "./adm": "./src/adm/index.js",
    "./apollo": "./src/apollo/index.js",
    "./auth": "./src/auth/index.js"
  },
  "scripts": {
    "dev": "yarn build && npx sls offline --stage local",
    "build": "yarn generate && tsc -b ./tsconfig.json",
    "dc:up": "docker-compose up -d",
    "generate": "yarn nexus:generate && yarn prisma:generate",
    "clean": "rm -rf .build",
    "test": "yarn generate && jest",
    "test:cov": "yarn test -- --coverage",
    "lint": "npx eslint \"src/**/*.ts\" --fix",
    "prepare": "husky install",
    "nexus:generate": "ts-node --transpile-only -e 'import NexusSchema from \"./src/apollo/schema.ts\"; NexusSchema.makeSchema()'",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:deploy": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@swc/core": "^1",
    "@swc/jest": "^0",
    "@types/aws-lambda": "^8",
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.1.1",
    "@types/lodash": "^4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "create-ts-index": "^1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.1",
    "jest": "^29",
    "prettier": "^3.1.0",
    "serverless": "^3.22.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.3.0",
    "serverless-plugin-monorepo": "^0.11.0",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-node": "^10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@as-integrations/aws-lambda": "^3.0.0",
    "@hubspot/api-client": "^10.1.0",
    "@prisma/client": "^5.6.0",
    "axios": "^1.4.0",
    "chance": "^1.1.9",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16",
    "graphql-scalars": "^1.19.0",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.3",
    "nexus": "^1",
    "nexus-shield": "^2.2.1",
    "prisma": "^5.6.0",
    "winston": "^3.8.2"
  }
