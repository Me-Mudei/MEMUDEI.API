  "scripts": {
    "prestart:lambda": "npm run build",
    "start:lambda": "npx sls offline --stage local",
    "prestart:apollo": "npm run build",
    "start:apollo": "npm run nexus:generate && ts-node-dev --no-notify --respawn --transpile-only src/apollo",
    "build": "npm run nexus:generate && tsc -b ./tsconfig.json",
    "build:w": "npm run nexus:generate && tsc -b ./tsconfig.json -w",
    "clean": "rm -rf .build",
    "clean:tsc": "rm tsconfig.tsbuildinfo",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "tsc:check": "npm run tsc -- --noEmit",
    "test": "npm run nexus:generate && jest",
    "test:cov": "npm run test -- --coverage",
    "cti": "cti",
    "cti:make": "sh ./cti.sh",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\" --fix",
    "prepare": "husky install",
    "nexus:generate": "ts-node --transpile-only -e 'import NexusSchema from \"./src/apollo/schema.ts\"; NexusSchema.makeSchema()'",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate"
  },
