  "imports": {
    "#apollo/*": "./src/apollo/*/index.js",
    "#next/*": "./src/next/*/index.js",
    "#shared/*": "./src/shared/*/index.js",
    "#property/*": "./src/property/*/index.js",
    "#adm/*": "./src/adm/*/index.js",
    "#auth/*": "./src/auth/*/index.js",
    "#user/*": "./src/user/*/index.js",
    "#schedule/*": "./src/schedule/*/index.js"
  },
  "exports": {
    "./apollo": "./src/apollo/index.js",
    "./nest": "./src/nest/index.js",
    "./shared": "./src/shared/index.js",
    "./user": "./src/user/index.js",
    "./schedule": "./src/schedule/index.js",
    "./property": "./src/property/index.js",
    "./adm": "./src/adm/index.js",
    "./auth": "./src/auth/index.js"
  },
  "scripts": {
    "build": "nest build",
    "build:nexus": "yarn generate && tsc -b ./tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "yarn build && npx sls offline --stage local",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "generate": "yarn nexus:generate && yarn prisma:generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "tsc": "tsc --noEmit",
    "test": "yarn generate && jest --detectOpenHandles --forceExit",
    "test:cov": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "nexus:generate": "ts-node --transpile-only -e 'import NexusSchema from \"./src/apollo/schema.ts\"; NexusSchema.makeSchema()'",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma migrate reset --force && prisma db push --force",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "prepare": "husky install",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject ./dist && sentry-cli sourcemaps upload ./dist"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
